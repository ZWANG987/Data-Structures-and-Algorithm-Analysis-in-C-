//令k=N/2，显示程序对N取不同值时的运行时间。
#include<iostream>
#include<ctime>
using namespace std;
int main()
{
    clock_t start=clock();
    for (int i=10000;i<150000;i+=500)
    {
        int k=i/2;
        cout<<k<<endl;
    }
    clock_t end=clock();
    double m=double(end-start);
    cout<<"Running Time: "<<m/CLOCKS_PER_SEC<<endl;
    return 0;
}
