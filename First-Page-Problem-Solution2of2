#include<iostream>
#include<algorithm>
using namespace std;
int main()
{
    int n,k;
    cin>>n>>k;
    int a[k];
    for (int i=0;i<k;i++)
    {
        cin>>a[i];

    }
    sort(a,a+k,greater<int>());
    for (int p=k;p<n;p++)
    {
        int m;
        cin>>m;
        if (m<=a[k-1])
        {
            continue;
        }
        for (int j=k-1;j>=0;j--)
        {
            if (m>a[j])
            {
                for (int l=k-1;l>j;l--)
                {
                    a[l]=a[l-1];
                }
                a[j]=m;
            }
        }
    }
    cout<<a[0]<<endl;
    return 0;
}
